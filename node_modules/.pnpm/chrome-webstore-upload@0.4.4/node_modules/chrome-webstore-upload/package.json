{
  "name": "chrome-webstore-upload",
  "version": "0.4.4",
  "license": "MIT",
  "description": "Programatically upload + publish changes to the Chrome Webstore",
  "repository": "DrewML/chrome-webstore-upload",
  "engines": {
    "node": ">=10.19.0"
  },
  "scripts": {
    "test": "ava"
  },
  "keywords": [
    "chrome",
    "publish",
    "webstore",
    "extension",
    "deploy",
    "update",
    "upload"
  ],
  "author": "Andrew Levine",
  "dependencies": {
    "got": "11.5.2"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "sinon": "^9.0.3",
    "xo": "^0.33.0"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": "off",
      "func-names": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": "off"
    }
  }
}
