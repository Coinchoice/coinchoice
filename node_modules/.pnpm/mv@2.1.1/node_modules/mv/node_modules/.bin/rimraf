#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@2.4.5/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@2.4.5/node_modules/rimraf/bin.js" "$@"
fi
