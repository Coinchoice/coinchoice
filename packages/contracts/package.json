{
	"name": "coinchoice-contracts",
	"version": "0.1.0",
	"description": "CCC",
	"engines": {
		"node": ">= 12.18.0"
	},
	"repository": "N/A",
	"author": "Achthar",
	"license": "MIT",
	"keywords": [
		"ethereum",
		"smart-contracts",
		"template",
		"boilerplate",
		"hardhat",
		"solidity"
	],
	"devDependencies": {
		"@nomiclabs/hardhat-ethers": "^2.0.2",
		"@nomiclabs/hardhat-etherscan": "^2.1.2",
		"@nomiclabs/hardhat-waffle": "^2.0.1",
		"@openzeppelin/contracts": "^4.2.0",
		"@openzeppelin/contracts-upgradeable": "^4.2.0",
		"@typechain/ethers-v5": "^7.0.1",
		"@typechain/hardhat": "^2.3.0",
		"@types/chai": "^4.2.18",
		"@types/mocha": "^8.0.2",
		"@types/node": "^15.12.2",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"chai": "^4.2.0",
		"chai-ethers": "^0.0.1",
		"cross-env": "^7.0.2",
		"dotenv": "^10.0.0",
		"eslint": "^7.28.0",
		"eslint-config-prettier": "^8.3.0",
		"ethers": "^5.3.1",
		"fs-extra": "^10.0.0",
		"hardhat": "^2.6.3",
		"hardhat-contract-sizer": "^2.1.1",
		"hardhat-deploy": "^0.8.11",
		"hardhat-gas-reporter": "^1.0.4",
		"mocha": "^9.0.0",
		"mocha-chai-jest-snapshot": "^1.1.0",
		"prettier": "^2.3.2",
		"prettier-plugin-solidity": "^1.0.0-beta.13",
		"solhint": "^3.3.6",
		"solhint-plugin-prettier": "^0.0.5",
		"solidity-coverage": "^0.7.15",
		"ts-generator": "^0.1.1",
		"ts-node": "^10.0.0",
		"typechain": "^5.1.2",
		"typescript": "^4.3.2"
	},
	"scripts": {
		"setup": "node ./.setup.js && hardhat typechain",
		"lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
		"lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
		"format": "prettier --check \"**/*.{ts,js,sol}\"",
		"format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
		"compile": "hardhat compile",
		"void:deploy": "hardhat deploy",
		"test": "cross-env HARDHAT_DEPLOY_FIXTURE=true HARDHAT_COMPILE=true mocha --bail --recursive test --timeout 1000000",
		"gas": "cross-env REPORT_GAS=true hardhat test",
		"coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
		"dev": "hardhat node --watch --export contractsInfo.json",
		"local:dev": "hardhat --network localhost deploy --watch",
		"execute": "node ./_scripts.js run",
		"deploy": "node ./_scripts.js deploy",
		"export": "node ./_scripts.js export",
		"fork:execute": "node ./_scripts.js fork:run",
		"fork:deploy": "node ./_scripts.js fork:deploy",
		"fork:dev": "node ./_scripts.js fork:dev",
		"fork:node": "node ./_scripts.js fork:node",
		"fork:test": "node ./_scripts.js fork:test",
		"build": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby build"
	},
	"dependencies": {
		"@defi-wonderland/smock": "^2.0.7",
		"@nomiclabs/hardhat-solhint": "^2.0.0",
		"@uniswap/conedison": "^1.3.0",
		"@openzeppelin/contracts": "3.4.2-solc-0.7",
		"@tenderly/hardhat-tenderly": "^1.0.12",
		"@types/lodash": "^4.14.177",
		"@uniswap/lib": "^4.0.1-alpha",
		"@uniswap/v2-core": "1.0.1",
		"base64-sol": "1.0.1",
		"decimal.js": "^10.3.1",
		"ethereum-waffle": "^3.0.0",
		"hardhat-abi-exporter": "^2.2.1",
		"hardhat-deploy-ethers": "^0.3.0-beta.10",
		"hardhat-preprocessor": "^0.1.4",
		"hardhat-spdx-license-identifier": "^2.0.3",
		"hardhat-typechain": "^0.3.5",
		"hardhat-watcher": "^2.1.1",
		"install": "^0.13.0",
		"is-svg": "^4.3.1",
		"lodash": "^4.17.21",
		"npm": "^7.20.6",
		"@uniswap/permit2-sdk": "1.2.0"
	}
}
